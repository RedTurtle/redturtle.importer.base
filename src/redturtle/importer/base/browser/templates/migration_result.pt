<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="">

  <body>
    <metal:content-title fill-slot="content-title">
      <metal:content-title define-macro="content-title">
        <h1 class="documentFirstHeading">Migration results</h1>
      </metal:content-title>
    </metal:content-title>
    <metal:content-core fill-slot="content-core">
      <metal:content-core define-macro="content-core"
        tal:define="results view/get_results">

        <h2>Contents migrated</h2>
        <table class="listing" summary="Results">
          <tbody>
            <tr>
                <th>In</th>
                <th>Out</th>
            </tr>
            <tr>
                <td>${results/in_count}</td>
                <td>${results/out_count}</td>
            </tr>
          </tbody>
        </table>
        <div class="errors" tal:condition="not:results/same_results">
          <h2>Contents not migrated</h2>
          <table class="listing" summary="Results">
            <tbody>
              <tr>
                <th>portal_type</th>
                <th>title</th>
                <th>path</th>
              </tr>
              <tr tal:repeat="item results/not_migrated">
                <td>${item/portal_type}</td>
                <td>${item/title}</td>
                <td>${item/path}</td>
                <td>${item/is_private}</td>
              </tr>
            </tbody>
          </table>
        </div>
      </metal:content-core>
    </metal:content-core>
  </body>
</html>
